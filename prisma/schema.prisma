generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Sex {
  M
  F
}

enum BarcodeType {
  PDF417
  CODE128
}

model Barcode {
  id        String   @id @default(uuid())
  url       String
  type      BarcodeType
  data      Json?
  userId    String
  editFlag  Boolean @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Address {
  id String @id @default(uuid())
  DAG String
  DAI String
  DAJ String
  DAK String
}

model Names {
  id String @id @default(uuid())
  DAC String
  DAD String
  DCS String
  DBC Sex
}